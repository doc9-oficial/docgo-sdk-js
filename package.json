{
    "name": "docgo-sdk",
    "version": "0.1.0",
    "description": "DocGo SDK for JavaScript / TypeScript",
    "license": "MIT",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "scripts": {
        "clean": "rm -rf dist",
        "build": "npm run clean && tsc --project tsconfig.json",
        "watch": "tsc --watch --project tsconfig.json",
        "prepare": "npm run build"
    },
    "exports": {
        ".": {
            "import": "./dist/src/index.js",
            "require": "./dist/src/index.js",
            "types": "./dist/src/index.d.ts"
        }
    },
    "dependencies": {
        "node-fetch": "^3.3.2"
    },
    "devDependencies": {
        "@types/node": "^20.11.0",
        "typescript": "^5.3.0"
    },
    "engines": {
        "node": ">=18"
    }
}